    "seed": "^0.4.4",
    "sqlite3": "^5.1.6",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/http-status-codes": "^1.2.0",
